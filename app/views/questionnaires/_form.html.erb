<%= form_for ([@questionnaire]), remote: true, :html => {:multipart => true} do |f| %>
	<p class="questionnaire-name">
		<%= f.text_field :name %>
	</p>

	<p>
		<%= f.label :description %>
		<%= f.text_area :description %>
	</p>

	<p>
		<%= f.label :public %>
		<%= f.check_box :public %>
	</p>

	<p>
		<%= submit_tag("Spara") %>
	</p>
<% end %>